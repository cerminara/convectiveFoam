FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

// Laminar viscosity
nu              [0 2 -1 0 0 0 0] 1e-1;

// Thermal expansion coefficient
beta            [0 0 0 -1 0 0 0] 1e-3;

// Reference temperature
TRef            [0 0 0 1 0 0 0] 0;

// Laminar thermal diffusivity
alpha           [0 2 -1 0 0 0 0] 1e-4;

//Gravity
g0              [0 1 -2 0 0 0 0] (0 0 -5000);
gamma           [0 -1 0 0 0 0 0] -1;

